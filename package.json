{
  "name": "modus-l-map",
  "version": "1.0.0",
  "author": "Modus Create <contact@moduscreate.com> (http://moduscreate.com)",
  "repository": "github:ModusCreateOrg/",
  "description": "StencilJS custom map web components with LeafletJS",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "license": "MIT",
  "private": true,
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "jest --no-cache",
    "test.watch": "jest --watch"
  },
  "dependencies": {
    "leaflet": "^1.3.4",
    "esri-leaflet": "^2.2.3"
  },
  "devDependencies": {
    "@stencil/core": "0.12.4",
    "@types/jest": "^23.3.1",
    "@types/leaflet": "^1.2.11",
    "@types/esri-leaflet": "^2.1.4",
    "jest": "^23.4.2"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
